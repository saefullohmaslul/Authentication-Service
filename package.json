{
  "name": "authentication-service",
  "version": "0.0.1",
  "description": "basic authentication service",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "cross-env DEBUG=server* ts-node ./src/index.ts",
    "start:docker": "docker-compose up",
    "start:watch": "tsc -w & nodemon --legacy-watch",
    "start:prod": "cross-env DEBUG=server* NODE_ENV=production npm-run-all clean build start",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand --verbose --coverage --forceExit --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand --verbose --coverage --forceExit --passWithNoTests --watchAll",
    "migrate:local": "cross-env NODE_ENV=development sh scripts/migrate.sh",
    "seeder:create": "cross-env NODE_ENV=development sh scripts/seeder.sh",
    "prepush": "npm run test",
    "typeorm": "typeorm --config dist/app/config/db/db-init.config.js",
    "migration:create": "node scripts/migration.js",
    "seed:run": "node ./dist/database/seeds/index.js",
    "seed": "node ./node_modules/typeorm-seeding/dist/cli.js --config dist/app/config/db/db-init.config.js seed"
  },
  "_moduleAliases": {
    "schema": "./dist/schema",
    "app": "./dist/app",
    "utils": "./dist/utils",
    "database": "./dist/database",
    "controllers": "./dist/controllers",
    "services": "./dist/services",
    "repositories": "./dist/repositories",
    "helpers": "./dist/helpers",
    "routes": "./dist/routes"
  },
  "keywords": [
    "microservice",
    "authentication"
  ],
  "author": "Saefulloh Maslul",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.1"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.10",
    "@types/node": "^13.7.7",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.0",
    "body-parser": "^1.19.0",
    "capitalize": "^2.0.2",
    "cross-env": "^7.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tslib": "^1.11.1",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.2.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2"
  }
}
